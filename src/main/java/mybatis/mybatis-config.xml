<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration
PUBLIC "-//mybatis.org//DTD Config 3.9//EN"
"http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
<settings>
<setting name="jdbcTypeForNull" value="NULL"/>
</settings>
   <typeAliases>
      <typeAlias type="model.Amem" alias="amem" />
      <typeAlias type="model.Auction" alias="auction" />
      <typeAlias type="model.Comment" alias="comment" />
      <typeAlias type="model.Cart" alias="cart" />
      <typeAlias type="model.Notice" alias="notice" />
      <typeAlias type="model.Report" alias="report"/>
      <typeAlias type="model.AddbuyList" alias="addbuyList"/>
        <typeAlias type="model.Apay" alias="apay" />
   </typeAliases>

   <environments default="development">
      <environment id="development">
         <transactionManager type="JDBC" />
         <dataSource type="POOLED">
            <property name="driver" value="oracle.jdbc.OracleDriver" />
            <property name="url"
               value="jdbc:oracle:thin:@localhost:1521:xe" />
            <property name="username" value="kic" />
            <property name="password" value="1111" />
         </dataSource>
      </environment>
   </environments>

   <mappers>
      <mapper resource="mybatis/amem.xml" />
      <mapper resource="mybatis/board.xml" />
      <mapper resource="mybatis/cart.xml" />
      <mapper resource="mybatis/notice.xml" />
      <mapper class="mybatis.AMemberAnno" />
      <mapper class="mybatis.AReportAnno" />
   </mappers>
</configuration>